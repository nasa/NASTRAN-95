LABEL    TRLGLOOP $        
CASE     CASECC,/CASEYY/*TRAN*/S,N,REPEATT/S,N,NOLOOP1 $        
PARAM    //*MPY*/NCOL /0 /1 $        
TRLG     CASEYY,USETD,DLT,SLT,BGPDT,SIL,CSTM,TRL,DIT,GMD,GOD,,EST,MGG,/ 
         ,,PDT1,PD1,,TOL/ NOSET/NCOL $        
