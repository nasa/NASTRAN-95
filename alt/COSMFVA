         S,N,NOGO $        
COND     ERRORC1,NOGO $        
EQUIV    PXTRZ1,PXFZ1/NORO2 $        
COND     LBLRO2A,NORO2 $        
MPYAD    PXTRZ1,REORDER2, / PXFZ1 /0 $        
LABEL    LBLRO2A $        
EQUIV    PXFZ1,PXF1 $        
JUMP     LBLTRL3 $        
LABEL    LBLTRL2 $        
$ CYCIO = +1        
MPYAD    PD,REORDER1, / PDTRZ2 / 0 $        
CYCT1    PDTRZ2 /PXTRZ2,GCYCF3 /CTYPE/*FORE*/NTSTEPS/V,Y,LMAX/
         V,Y,NSEGS/S,N,NOGO $        
COND     ERRORC1,NOGO $        
EQUIV    PXTRZ2,PXTR2/NORO2 $        
COND     LBLRO2B,NORO2 $        
MPYAD    PXTRZ2,REORDER2, / PXTR2 /0 $        
LABEL    LBLRO2B $        
